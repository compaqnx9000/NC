.tabar {
  display: flex;
  justify-content: center;
  background-color: transparent;
  /* background-color: #7e267e; */
}

.tabar text {
  padding: 24rpx 30rpx;
  color: #353535;
  font-size: 12pt;
  font-weight: bold;
}

.tabar .active {
  color: #fff;
}
