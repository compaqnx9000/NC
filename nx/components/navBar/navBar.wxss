/* 顶部导航 */

.status-bar {
  width: 100%;
  z-index: 99999;
  position: fixed;
  top: 0;
}

.goBack {
  position: absolute;
  /* top: 66px; */
  font-size: 30px;
  /* left: 300rpx; */

  width: 64px;
  border: 1px solid hsla(0, 0%, 100%, 0.25);
  border-radius: 27px;
  background: hsla(0, 0%, 100%, 0.6);
  line-height: 30px;
  height:30px;
}

